{
  "name": "web-starter",
  "version": "1.0.0",
  "description": "Eddie's web starter",
  "main": "src/app.js",
  "scripts": {
    "clean": "rm -rf ./build ./coverage ./*.log",
    "test": "jest --coverage",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint ./src/css/*.css ./src/css/**/*.css --config ./.stylelintrc.json",
    "lint:js": "eslint ./src --config ./.eslintrc.json",
    "dev": "babel-node ./devServer.js",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --config ./webpack.config.build.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com:eddiewentw/web-starter.git"
  },
  "keywords": [
    "web",
    "starter"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "beta",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "ip": "^1.1.4",
    "jest": "^22.4.2",
    "postcss": "^5.2.6",
    "postcss-cssnext": "^2.9.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-extend": "^1.0.5",
    "postcss-loader": "^1.1.1",
    "postcss-mixins": "^5.4.0",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.13.1",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-webpack-plugin": "^0.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.1"
  },
  "author": "Eddie Wen <eddiewen.tw@gmail.com> (http://eddiewen.me)",
  "bugs": {
    "url": "https://github.com/eddiewentw/web-starter/issues"
  },
  "homepage": "https://github.com/eddiewentw/web-starter#readme",
  "license": "MIT"
}
